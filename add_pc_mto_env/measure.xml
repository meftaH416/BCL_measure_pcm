      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>schedule_name</name>
      <display_name>Schedule Name for Zone Mixing</display_name>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>design_level</name>
      <display_name>Design Level for Zone Mixing</display_name>
      <type>Double</type>
      <units>cfm</units>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
    <argument>
      <name>source_zone_name</name>
      <display_name>Source Zone for Zone Mixing</display_name>
      <type>String</type>
      <required>true</required>
      <model_dependent>false</model_dependent>
    </argument>
  </arguments>
